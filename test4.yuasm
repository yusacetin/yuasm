#define abc 0 // define stuff
#define def 1
#define ghi 2

loadimm abc, 10
loadimm def , 11
loaddir ghi ,def

.operations: // do some operations
add abc def ghi;
sub def ,abc def ;

. memory:
/* save things
to memory */
store 0xA000 abc /* like this */
store 0xA001 def//and this
store 0xA002, ghi;

